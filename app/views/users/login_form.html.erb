<body>
  <center><table>
    <%= form_with url: login_path, method: :post, local: true do |f| %>
      <tr>
        <td> <%= f.label "Email" %> </td>
        <td> <%= f.text_field :email %> </td>
      </tr>
      <tr>
        <td> <%= f.label :password, "Password" %> </td>
        <td> <%= f.password_field :password %> </td>
      </tr>
      <tr>
        <td> <%= f.submit "Log In" %> </td>
      </tr>
    <% end %>
  </table> </center>
</body>